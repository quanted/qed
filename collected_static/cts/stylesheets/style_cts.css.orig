/*************************************************
CTS-specific styles for overriding shared style.css
**************************************************/

html, body {
  font-family: 'Open Sans', sans-serif; /* changing */
  background-color: #F4F7ED;
}

h4 {
  font-weight: bold;
}

.articles_input {
  width: 822px;
  display: inline;
  float: left;
  position: relative;
  margin: 8px 0 8px 8px;
  padding: 12px 16px;
  background-color: #F7F9F4;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  box-shadow: 3px 3px 15px #CCC;
  -webkit-box-shadow: 3px 3px 15px #CCC;
  -moz-box-shadow: 3px 3px 15px #CCC;
}

.model_header {
  color: #4289AA;
  margin-bottom: 20px;
  font-size: 1.5em;
  font-weight: 700;
  display: inline-block;
}
.input_header {
  color: #000;
  margin-bottom: 1em;
  font-size: 1.2em;
  display: inline;
}
.popup_header {
  color: #000;
  margin-bottom: 1em;
  font-size: 1.5em;
  font-weight: 700;
}

/* Defining Hyperlink Colors */

a:link {
  color: #4289AA;
  text-decoration: none;
}
a:visited {
  color: #4289AA;
  text-decoration: none;
}
a:hover {
  color: #4289AA;
  text-decoration: none;
}
#header_menu_nav a {
  color: white;
  text-decoration: none;
}

/* 01uberheader.html */

#banner {
  width: 1024px;
  /*background: #356697 url(/static_qed/cts/images/epa_logo.png) no-repeat;*/
  background-color: #356697;
  /*min-height: 70px;*/
  min-height: 30px;
  font-size: 1.6em;
  border-bottom: 2px solid #1F262A;
  border-radius: 4px 4px 0 0;
  -webkit-border-radius: 4px 4px 0 0;
  -moz-border-radius: 4px 4px 0 0;
  /*box-shadow: 3px 3px 15px #CCC;
  -webkit-box-shadow: 3px 3px 15px #CCC;
  -moz-box-shadow: 3px 3px 15px #CCC;*/
}
#banner_l {
  color: #fff;
  float: left;
  /*padding: 46px 0 4px 13px;*/
  padding: 5px 0 0 0.4em;
}
#banner_r {
  float: right;
}
#banner_r a {
  color: #F9FBF6;
}
.textshadow {
  text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.4);
}
#topheader_pic {
  background: #356697 url(/static_qed/cts/images/species/cts_balls.png);
  width: 720px;
  height: 120px;
  float: left;
  border-right: 4px solid #1F262A;
  border-bottom: 2px solid #1F262A;
}
#topheader_p {
  width: 300px;
  height: 120px;
  background: #1F262A url(/static_qed/cts/images/epa_logo_gray.png) no-repeat center;
  float: right;
  color: white;
  /*font-size: 12px;*/
  text-align: justify;
  border-bottom: 2px solid #1F262A;
}
#topheader_p p {
  margin-left: auto;
  margin-right: auto;
  width: 240px;
  padding-top: 20%; /* Text Positioning */
}
#epaSkin {
  display: none;
}
#topheader_pic_main {
  background: #356697 url(/static_qed/cts/images/species/cts_balls.png);
  width: 720px;
  height: 240px;
  float: left;
  border-right: 4px solid #1F262A;
  border-bottom: 2px solid #1F262A;
}
#topheader_p_main {
  width: 300px;
  height: 240px;
  background-color: #1F262A;
  float: right;
  color: white;
  /*font-size: 12px;*/
  text-align: justify;
  border-bottom: 2px solid #1F262A;
}
#topheader_p_main p {
  margin-left: auto;
  margin-right: auto;
  width: 240px;
  padding-top: 20%; /* Text Positioning */
}
a.logreg:link, a.logreg:visited, a.logreg:active {
  color: #F9FBF6;
  font-size: 18px;
  margin-left: 10px;
  float: right;
}

